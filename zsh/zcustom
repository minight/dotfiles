lookup_psm () {
  pushd /Users/bytedance/bytedance/tools/cloud_sherlock
  /Users/bytedance/.local/share/virtualenvs/cloud_sherlock-7ZPtSwVC/bin/python codebase_from_psm.py $@
  popd
}

lookup_tlb () {
  pushd /Users/bytedance/bytedance/tools/cloud_sherlock
  /Users/bytedance/.local/share/virtualenvs/cloud_sherlock-7ZPtSwVC/bin/python codebase_from_url.py $@
  popd
}

clone_repo() {
  pushd $HOME/bytedance
  mkdir -p $1
  git clone "https://code.byted.org/$1" $1
  popd
}

